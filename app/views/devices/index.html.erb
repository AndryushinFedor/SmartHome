<div class=main>
<div class='searchdevice'>
<%= render 'search/form' %>
  <%= link_to image_tag("/images/adddevice.svg"), new_device_path %>
</div>
<div class='deviceTag'>
<% @tags.each do |tag| %>
  <span>
    <a href="/devices/?tags=<%= tag %>"><%= tag %></a>
  </span>
<% end %>
<span>
  <%= link_to 'all', devices_path %>
</span>
</div>

<div class='devFlex'>
<%= react_component 'devices/A_DevicesList', { devices: @devices } %>
</div>

</div>